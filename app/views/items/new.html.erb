<h1>new</h1>
<%= form_for(@item) do |f|  %>
  <div>
  <%= f.label :title %>
  <%= f.text_field :title %>
  </div>

  <div>
  <%= f.label :description %>
  <%= f.text_area :description %>
  </div>

  <div>
  <%= f.label :price %>
  <%= f.text_area :price %>
  </div>

  <div>
  <%= f.select(:deal, [["팝니다", 0], ["삽니다", 1], ["나눔합니다", 2]])  %>
  </div>

  <%= f.submit "SELL ITEM"  %>
<% end %>
